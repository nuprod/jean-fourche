<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <template id="nuprod_production_template_zpl">
            <t t-set="uom_categ_unit" t-value="env.ref('uom.product_uom_categ_unit')"/>
            <t t-foreach="docs" t-as="production">
                <t t-translation="off">
                <t t-set="lineHeight" t-value="50"/>
^XA^CI28
^FO150,<t t-out="lineHeight"/>
^A0N,44,33^FD<t t-esc="production.product_id.name"/>
^FS
<t t-set="lineHeight" t-value="lineHeight + 50"/>
<t t-if="production.product_id.product_template_attribute_value_ids">
^FO150,<t t-out="lineHeight"/>
^A0N,44,33^FD<t t-out="production.product_id.product_template_attribute_value_ids._get_combination_name()"/>
^FS
<t t-set="lineHeight" t-value="lineHeight + 50"/>
</t>
<t t-if="production.sale_order_id">
^FO150,<t t-out="lineHeight"/>
^A0N,44,33^FDClient: <t t-esc="production.sale_order_id.partner_id.display_name"/>
^FS
<t t-set="lineHeight" t-value="lineHeight + 50"/>
</t>
<t t-if="production.lot_producing_id">
^FO150,<t t-out="lineHeight"/>
^A0N,44,33^FDLN/SN: <t t-esc="production.lot_producing_id.name"/>
^FS
^FO15,50^BY3
^BON,4,N,104
^FD<t t-esc="production.lot_producing_id.name"/>
^FS
<t t-set="lineHeight" t-value="lineHeight + 50"/>
</t>
<t t-else="">
^FO150,<t t-out="lineHeight"/>
^A0N,44,33^FDNo barcode available^FS
<t t-set="lineHeight" t-value="lineHeight + 50"/>
</t>
^XZ
                </t>
            </t>
</template>
</data>
</odoo>
