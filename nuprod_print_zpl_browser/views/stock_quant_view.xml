<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>

    <record id="nuprod_stock_quant_print_labels_zpl" model="ir.ui.view">
        <field name="name">nuprod.zpl.stock.quant</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_tree_editable"/>
        <field name="arch" type="xml">
            <xpath expr="//header/button[@name='action_stock_quant_relocate']" position='after'>
                <button name="action_nuprod_print_quant_zpl" string="Print ZPL" type="object" class="btn-primary ms-1" icon="fa-print" groups="stock.group_stock_manager" invisible="context.get('hide_location', False)" context="{'action_ref': 'stock.action_view_quants'}"/>
            </xpath>
        </field>
    </record>

  </data>
</odoo>